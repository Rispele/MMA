        </PackageReference>
        <PackageReference Include="NUnit3TestAdapter" Version="6.0.0" />
        <PackageReference Include="SkbKontur.NUnit.Middlewares" Version="0.1.11" />
    </ItemGroup>

    <ItemGroup>
        <Using Include="NUnit.Framework"/>
    </ItemGroup>

    <ItemGroup>
